      <MemberSignature Language="ILAsm" Value=".method public hidebysig instance void GetData(class Microsoft.Health.HealthRecordAccessor record, int32 startIndex, int32 count) cil managed" />
      <MemberSignature Language="DocId" Value="M:Microsoft.Health.HealthRecordItemDataTable.GetData(Microsoft.Health.HealthRecordAccessor,System.Int32,System.Int32)" />
      <MemberType>Method</MemberType>
