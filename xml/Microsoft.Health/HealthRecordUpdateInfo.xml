    <Member MemberName="LatestOperationSequenceNumber">
      <MemberSignature Language="C#" Value="public long LatestOperationSequenceNumber { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance int64 LatestOperationSequenceNumber" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Health.HealthRecordUpdateInfo.LatestOperationSequenceNumber" />
      <MemberType>Property</MemberType>
