<aerobic-weekly>
	<session>
		<mode>
			<text>running</text>
			<code>
				<value>run</value>
				<family>wc</family>
				<type>aerobic-activities</type>
				<version>1</version>
			</code>
		</mode>
		<distance>
			<m>400</m>
			<display units="400 m">00</display>
		</distance>
		<minutes>10</minutes>
		<intensity>4</intensity>
		<peak-heartrate>102</peak-heartrate>
		<avg-heartrate>78</avg-heartrate>
		<min-heartrate>69</min-heartrate>
		<energy>1000</energy>
		<energy-from-fat>40</energy-from-fat>
		<peak-speed>
			<meters-per-second>4</meters-per-second>
			<display units="4 m/sec">4</display>
		</peak-speed>
		<avg-speed>
			<meters-per-second>2</meters-per-second>
			<display units="2 m/sec">2</display>
		</avg-speed>
		<min-speed>
			<meters-per-second>1</meters-per-second>
			<display units="1 m/sec">1</display>
		</min-speed>
		<peak-pace>
			<seconds-per-hundred-meters>400</seconds-per-hundred-meters>
			<display units="400 sec/100m">400</display>
		</peak-pace>
		<avg-pace>
			<seconds-per-hundred-meters>200</seconds-per-hundred-meters>
			<display units="200 sec/100m">200</display>
		</avg-pace>
		<min-pace>
			<seconds-per-hundred-meters>100</seconds-per-hundred-meters>
			<display units="100 sec/100m">100</display>
		</min-pace>
		<peak-power>
			<watts>40</watts>
			<display units="40">40</display>
		</peak-power>
		<avg-power>
			<watts>30</watts>
			<display units="30">30</display>
		</avg-power>
		<min-power>
			<watts>20</watts>
			<display units="20">20</display>
		</min-power>
		<peak-torque>
			<newton-meters>400</newton-meters>
			<display units="400">400</display>
		</peak-torque>
		<avg-torque>
			<newton-meters>200</newton-meters>
			<display units="">200</display>
		</avg-torque>
		<min-torque>
			<newton-meters>100</newton-meters>
			<display units="100">100</display>
		</min-torque>
		<left-right-balance>1</left-right-balance>
		<peak-cadence>3</peak-cadence>
		<avg-cadence>2</avg-cadence>
		<min-cadence>1</min-cadence>
		<peak-temperature>
			<celsius>110</celsius>
			<display units="">110</display>
		</peak-temperature>
		<avg-temperature>
			<celsius>100</celsius>
			<display units="">100</display>
		</avg-temperature>
		<min-temperature>
			<celsius>96</celsius>
			<display units="">96</display>
		</min-temperature>
		<peak-altitude>
			<m>5</m>
			<display units="">5</display>
		</peak-altitude>
		<avg-altitude>
			<m>2</m>
			<display units="">2</display>
		</avg-altitude>
		<min-altitude>
			<m>1</m>
			<display units="">1</display>
		</min-altitude>
		<elevation-gain>
			<m>5</m>
			<display units="">5</display>
		</elevation-gain>
		<elevation-loss>
			<m>3</m>
			<display units="">3</display>
		</elevation-loss>
		<number-of-steps>200</number-of-steps>
		<number-of-aerobic-steps>400</number-of-aerobic-steps>
		<aerobic-step-minutes>60</aerobic-step-minutes>
	</session>
	<recurrence>2</recurrence>
</aerobic-weekly>
