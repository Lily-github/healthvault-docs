<defibrillator-episode>
    <when>
        <date>
            <y>2013</y>
            <m>10</m>
            <d>28</d>
        </date>
    </when>
    <episode-type-group>
        <text>SVT/NST</text>
    </episode-type-group>
    <episode-type>
        <text>NST</text>
    </episode-type>
    <data-source>
        <text>paceart</text>
    </data-source>
    <duration-in-seconds>2</duration-in-seconds>
    <episode-fields>
        <episode-field>
            <field-name>
                <text>Episode In Progress</text>
            </field-name>
            <field-value>
                <text>False</text>
            </field-value>
        </episode-field>
        <episode-field>
            <field-name>
                <text>Reason</text>
            </field-name>
            <field-value>
                <text>Non sustained</text>
            </field-value>
        </episode-field>
        <episode-field>
            <field-name>
                <text>Number of Beats</text>
            </field-name>
            <field-value>
                <text>9</text>
            </field-value>
        </episode-field>            
    </episode-fields>
</defibrillator-episode>