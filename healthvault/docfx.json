{
    "build": {
        "content": [{
            "files": [
                "**/*.md",
                "**/*.swagger.json"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**",
                "README.md",
                "LICENSE",
                "LICENSE-CODE"
            ]
        }],
        "resource": [{
            "files": [
                "**/*.png",
                "**/*.jpg"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**"
            ]
        }],
        "overwrite": [],
        "externalReference": [],
        "globalMetadata": {
            "titleSuffix": "Microsoft HealthVault Development"
        },
        "fileMetadata": {},
        "template": [],
        "dest": "healthvault"
    }
}