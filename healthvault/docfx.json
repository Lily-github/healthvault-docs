{
    "build": {
        "content": [{
            "files": [
                "**/*.md",
              "**/*.swagger.json",
              "**/*.yml"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**",
                "README.md",
                "LICENSE",
                "LICENSE-CODE"
            ]
        }],
        "resource": [{
          "files": [
            "**/*.png",
            "**/*.jpg",
            "**/*.svg",
            "**/*.xsd"
          ],
            "exclude": [
                "**/obj/**",
                "**/includes/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "**/*-overwrite.md"
            ], 
            "exclude": [
                "**/obj/**",
                "**/includes/**"
            ]
        }],
        "externalReference": [],
        "globalMetadata": {
            "titleSuffix": "HealthVault Development",
            "uhfHeaderId": "MSDocsHeader-HealthVault",
            "ms.prod": "healthvault"
        },
        "fileMetadata": {},
        "template": [],
        "dest": "healthvault"
    }
}