{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "src":"docs-ref-autogen",
        "files": [
          "**.json",
          "toc.md"
        ],
        "dest":"rest-api"
      },
      {
        "src":"docs-ref-conceptual",
        "files": [
          "**.md"
        ],
        "exclude": [
          "**/toc.md"
        ],
        "dest":"rest-api"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.xsd"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [],
    "dest": "healthvault"
  }
}