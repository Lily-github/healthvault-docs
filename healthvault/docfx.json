{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.swagger.json",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "api/overwrites/**",
          "sdks/java/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.xsd"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**/*-overwrite.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "titleSuffix": "HealthVault Development",
      "uhfHeaderId": "MSDocsHeader-HealthVault",
      "ms.prod": "healthvault"
    },
    "template": [],
    "dest": "healthvault"
  }
}