{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "api/overwrites/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/overwrites/**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "titleSuffix": "HealthVault Development",
      "uhfHeaderId": "MSDocsHeader-HealthVault",
      "ms.prod": "healthvault",
      "apiPlatform": "dotnet"
    },
    "fileMetadata": {
      "author": {
        "api/**/*.yml": "dotnet-bot"
      }
    },
    "template": [],
    "xref": [
      "msdn.4.5.2.zip"
    ],
    "dest": "healthvault-dotnet",
    "markdownEngineName": "markdig"
  }
}
